set -e

. common/setup-java-prepackaging.sh



cd ${BUILD_DIR}/che
mvn -X -B -U -e clean install -DskipTests=true -Pskip-git-info  --settings settings.xml 
cp -R assembly/assembly-main/target/eclipse-che-${che_version}/eclipse-che-${che_version}  ${BUILD_DIR}/che


cd ${BUILD_DIR}
rm -rf che

cleanup_java

